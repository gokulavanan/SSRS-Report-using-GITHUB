  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>TMB</DataSourceName>
        <CommandText>SELECT     rtrim(SOP10100.CUSTNMBR) AS [Customer Num], rtrim(SOP10100.CUSTNAME) AS [Customer Name], SOP10100.DOCDATE AS [Doc Date], 
                         rtrim(SOP10200.SOPNUMBE) AS [Sop Num],case when  SOP10200.SOPTYPE=2 then 'Order' end  AS [Sop type], case rtrim(SOP10100.PSTGSTUS) when 2 then 'Posted'  when 0 then 'UnPosted' end AS [Doc Status], rtrim(IV00101.ITEMNMBR) AS [Item Number], 
                         rtrim(IV00101.ITEMDESC) AS [Item Desc], rtrim(IV00101.ITMSHNAM) AS [Item Short Name], SOP10200.QUANTITY AS QTY, SOP10200.QTYREMAI AS [QTY Remaining], SOP10200.LNITMSEQ AS [LIne Sequence],
                         SOP10200.UNITPRCE AS [Unit Price], SOP10200.XTNDPRCE AS [Extended Price], SOP10106.USRDAT01 AS [Cancel Date], 
                         SOP10200.ReqShipDate AS [Requseted Ship Date], SOP10200.ACTLSHIP AS [Actual Ship Date], rtrim(SOP10100.BACHNUMB) AS [Batch Number], 
                         rtrim(RM00101.CUSTCLAS) AS [Customer Class], rtrim(IV00101.USCATVLS_1) AS League, rtrim(IV00101.USCATVLS_4) AS Team, 
                         rtrim(ITEMCATEGORY.[PLANNING CATEGORY 1]) AS [Planning Category1], rtrim(ITEMCATEGORY.[PLANNING CATEGORY 2]) AS [Planning Category2], 
                         rtrim(ITEMCATEGORY.[PLANNING CATEGORY 3]) AS [Planning Category3],rtrim(SOP10200.SLPRSNID) as SalesPersonID

from SOP10200 inner join 
	 SOP10100 on SOP10200.SOPNUMBE = SOP10100.SOPNUMBE and SOP10200.SOPTYPE = SOP10100.SOPTYPE
	 Left outer join SOP10106 on SOP10200.SOPNUMBE = SOP10106.SOPNUMBE and SOP10200.SOPTYPE = SOP10106.SOPTYPE
	 left outer join ITEMCATEGORY on ITEMCATEGORY.[Item Number] = SOP10200.ITEMNMBR
	 Inner join IV00105 on 	SOP10200.ITEMNMBR = IV00105.ITEMNMBR
	 inner join IV00101 on SOP10200.ITEMNMBR = IV00101.ITEMNMBR
	 inner join RM00101 on RM00101.CUSTNMBR = SOP10100.CUSTNMBR

where
SOP10100.CUSTNMBR &lt;&gt; 'TEAMB0001' and
SOP10200.QTYREMAI &gt; 0 and
SOP10200.SOPTYPE =2 AND
SOP10100.PSTGSTUS =0 and 
SOP10100.VOIDSTTS =0</CommandText>
      </Query>
